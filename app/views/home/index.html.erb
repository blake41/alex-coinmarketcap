<% @data.each_pair do |week, week_data| %>
  week: <%= week %><br>
  rois:<br>
  <table>
    <tr>
      <th>currency id</th>
      <th>percent gain</th>
    </tr>
  <% week_data['rois'].each_pair do |currency_id, roi| %>
    <tr>
      <td>
        <%= @map[currency_id.to_i] %>
      </td>
      <td>
        <%= number_to_percentage(roi * 100) %>
      </td>
    </tr>
  <% end %>
  </table>
  amounts:<br>
  <table>
    <tr>
      <th>currency id</th>
      <th>amount</th>
    </tr>
  <% week_data['amounts'].each_pair do |currency_id, amount| %>
    <tr>
      <td>
        <%= @map[currency_id.to_i] %>
      </td>
      <td>
        <%= number_to_currency(amount, :delimiter => ',' , :precision => 2) %>
      </td>
    </tr>
  <% end %>
  </table>
  <br>
  sum:<%= number_to_currency(week_data['sum'], :delimiter => ',', :precision => 2) %>
  <br>
<% end %>
